pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh '/usr/local/Cellar/maven/3.6.3_1/libexec/bin/mvn clean install'
            }
        }
        stage('Create docker image') {
            steps {
                sh 'docker image -t sapkalrohit007/spring-boot .'
            }
        }
    }
}
